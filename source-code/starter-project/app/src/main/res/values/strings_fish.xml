    <!-- Seared Ahi Tuna Recipe (ID 20) -->
    <string name="fish_seared_ahi_tuna_name">Seared Ahi Tuna</string>
    <string name="fish_seared_ahi_tuna_description">Seared rare tuna with sesame crust and wasabi sauce</string>
    <string name="fish_seared_ahi_tuna_ingredient_1">4 ahi tuna steaks (6 ounces each)</string>
    <string name="fish_seared_ahi_tuna_ingredient_2">1/4 cup sesame seeds</string>
    <string name="fish_seared_ahi_tuna_ingredient_3">1/4 cup soy sauce</string>
    <string name="fish_seared_ahi_tuna_ingredient_4">1/4 cup wasabi sauce</string>
    <string name="fish_seared_ahi_tuna_ingredient_5">2 tablespoons olive oil</string>
    <string name="fish_seared_ahi_tuna_ingredient_6">Salt and pepper to taste</string>

    <string name="fish_seared_ahi_tuna_instruction_1">1. Heat oil in a large skillet over high heat</string>
    <string name="fish_seared_ahi_tuna_instruction_2">2. Add tuna steaks and sear for 1-2 minutes on each side</string>
    <string name="fish_seared_ahi_tuna_instruction_3">3. Brush the tuna with soy sauce and wasabi sauce</string>
    <string name="fish_seared_ahi_tuna_instruction_4">4. Sprinkle sesame seeds on top of the tuna</string>
    <string name="fish_seared_ahi_tuna_instruction_5">5. Serve hot</string>
